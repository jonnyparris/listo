{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "svelte": {
      "type": "remote",
      "url": "https://mcp.svelte.dev/mcp",
      "enabled": true
    },
    "chrome-devtools": {
      "type": "local",
      "command": ["npx", "chrome-devtools-mcp@latest"],
      "enabled": true
    },
    "cloudflareDocs": {
      "type": "local",
      "command": ["npx", "mcp-remote", "https://docs.mcp.cloudflare.com/sse"],
      "enabled": true
    }
  }
}
